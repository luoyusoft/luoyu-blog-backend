<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.jinhx.blog.mapper.operation.TagLinkMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.jinhx.blog.entity.operation.TagLink">
        <id column="id" property="id" />
        <result column="tag_id" property="tagId" />
        <result column="link_id" property="linkId" />
        <result column="module" property="module" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <!-- 查询所有结果列 -->
    <sql id="AllColumn">
        id, tag_id, link_id, module,create_time,update_time
    </sql>

</mapper>
